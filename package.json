{
  "name": "universal",
  "description": "UMD grunt-init template",
  "version": "0.1.0",
  "homepage": "https://github.com/ryanve/universal",
  "license": "MIT",
  "author": "Ryan Van Etten",
  "keywords": ["umd", "template", "boilerplate"],
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanve/universal.git"
  },
  "devDependencies": {
    "grunt-contrib-jshint": "~0.9.0"
  },
  "jshintConfig": {
    "boss": true,
    "browser": true,
    "debug": true,
    "devel": true,
    "eqnull": true,
    "evil": true,
    "expr": true,
    "globals": {
      "define": true,
      "ender": true 
    },
    "jquery": true,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxerr": 10,
    "node": true,
    "sub": true,
    "supernew": true,
    "undef": true,
    "unused": true
  }
}