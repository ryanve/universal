{
  "private": true,
  "name": "{%= name %}",
  "description": "{%= name %}",
  "version": "{%= version %}",
  "homepage": "{%= homepage %}",
  "license": "{%= license %}",
  "author": "{%= author %}",
  "keywords": [],
  "main": "{%= main %}",
  "repository": {
    "type": "{%= repository.type %}",
    "url": "{%= repository.url %}"
  },
  "dependencies": {},
  "optionalDependencies": {},
  "devDependencies": {
    "aok": "~1.8.1",
    "grunt-contrib-uglify": "~0.4.0",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-jshint": "~0.9.0"
  },
  "jshintConfig": {
    "asi": false,
    "boss": true,
    "browser": true,
    "debug": true,
    "devel": true,
    "eqnull": true,
    "evil": true,
    "expr": true,
    "globals": {
      "define": true,
      "ender": true 
    },
    "jquery": true,
    "lastsemic": true,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxcomplexity": 15,
    "maxerr": 10,
    "maxlen": 115,
    "maxparams": 4,
    "node": true,
    "sub": true,
    "supernew": true,
    "trailing": true,
    "undef": true,
    "unused": true,
    "worker": true
  }
}